#include<stdio.h>

int main() 
{
	int age , marks;
	printf(" enter your age\n");
	scanf("%d",&age);
	printf(" enter your marks\n");
	scanf("%d", &marks);
	
	switch(age)
	{
	case 3:
	printf("The age is 3\n");
	break;
	switch(marks)
	{
	case 45:
	 printf("your marks is 45\n");
    break;
    default:
    	printf("your marks is not 45\n ");
         break;
}
	case 13:
    printf("The age is 13\n");
    break;
    default:
    printf("the age is not 3 and 13\n");
}
	return 0;
	
}
